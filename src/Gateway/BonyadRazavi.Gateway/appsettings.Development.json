{
  "Security": {
    "AllowedCidrs": [ "192.168.103.0/27" ],
    "ApiAllowPrefixes": [ "/api/auth" ],
    "AllowLoopbackInDevelopment": true
  },
  "ReverseProxy": {
    "Clusters": {
      "auth-api-cluster": {
        "Destinations": {
          "auth-api": {
            "Address": "http://localhost:5254/"
          }
        }
      },
      "web-app-cluster": {
        "Destinations": {
          "web-app": {
            "Address": "http://localhost:5079/"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}

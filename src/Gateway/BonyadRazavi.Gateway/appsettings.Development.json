{
  "Security": {
    "AllowedCidrs": [ "192.168.93.0/27" ],
    "ApiAllowPrefixes": [ "/api/auth", "/api/account", "/api/users", "/api/audit", "/api/companies" ],
    "AllowLoopbackInDevelopment": true,
    "ValidateJwtAtGateway": false
  },
  "ReverseProxy": {
    "Clusters": {
      "auth-api-cluster": {
        "Destinations": {
          "auth-api": {
            "Address": "http://localhost:5254/"
          }
        }
      },
      "web-app-cluster": {
        "Destinations": {
          "web-app": {
            "Address": "http://localhost:5079/"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}

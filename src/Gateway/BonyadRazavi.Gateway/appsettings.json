{
  "ReverseProxy": {
    "Routes": {
      "auth-route": {
        "ClusterId": "auth-api-cluster",
        "Order": 0,
        "Match": {
          "Path": "/api/auth/{**catch-all}"
        }
      },
      "web-route": {
        "ClusterId": "web-app-cluster",
        "Order": 100,
        "Match": {
          "Path": "/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "auth-api-cluster": {
        "Destinations": {
          "auth-api": {
            "Address": "https://localhost:7115/"
          }
        }
      },
      "web-app-cluster": {
        "Destinations": {
          "web-app": {
            "Address": "https://localhost:7197/"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
